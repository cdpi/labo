{
"compilerOptions":
	{
	"target": "esnext",
	"module": "esnext",
	"rootDir": "./src",
	"outDir": "./js",
	"esModuleInterop": false,
	"allowSyntheticDefaultImports": true,
	"lib": ["DOM", "ESNext"],
	"moduleResolution": "node",
	"forceConsistentCasingInFileNames": true,
	"strict": true,
	"skipLibCheck": true,
	"experimentalDecorators": true,
	"emitDecoratorMetadata": true,
	"strictPropertyInitialization": false,
	}
}