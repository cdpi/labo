{
"version": "2.0.0",
"tasks":
	[
		{
		"label": "OpenSCAD: Exporter en STL",
		"type": "shell",
		"command": "openscad",
		"args":
			[
				"-o", "${fileDirname}/${fileBasenameNoExtension}.stl",
				"${file}"
			],
		"group":
			{
			"kind": "build",
			"isDefault": true
			},
		"presentation":
			{
			"reveal": "silent",
			"revealProblems": "onProblem",
			"panel": "shared",
			"showReuseMessage": false,
			"clear": true
			}
		}
	]
}