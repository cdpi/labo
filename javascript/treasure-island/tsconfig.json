{
"compilerOptions":
	{
	"target": "esnext",
	"module": "esnext",
	"rootDir": "./src",
	"outDir": "./dist",
	"baseUrl": ".",
	"paths": {
		"@cdpi/binding/*": ["node_modules/@cdpi/common/dist/binding/*"],
		"@cdpi/geometry/*": ["node_modules/@cdpi/common/dist/geometry/*"],
		"@cdpi/css/*": ["node_modules/@cdpi/browser/dist/css/*"],
		"@cdpi/svg/*": ["node_modules/@cdpi/browser/dist/svg/*"]
	},
	"declaration": true,
	"esModuleInterop": false,
	"allowSyntheticDefaultImports": true,
	"lib": ["DOM", "ESNext"],
	"moduleResolution": "node",
	"forceConsistentCasingInFileNames": true,
	"strict": true,
	"skipLibCheck": true,
	"experimentalDecorators": true,
	"emitDecoratorMetadata": true,
	"strictPropertyInitialization": true
	}
}