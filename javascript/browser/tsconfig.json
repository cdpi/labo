{
"compilerOptions":
	{
	"target": "esnext",
	"module": "esnext",
	"rootDir": "./src",
	"outDir": "./dist",
	"baseUrl": ".",
	"paths": {
		"@cdpi/binding/*": ["node_modules/@cdpi/common/dist/binding/*"],
		"@cdpi/util/*": ["node_modules/@cdpi/common/dist/util/*"],
		"@cdpi/geometry/*": ["node_modules/@cdpi/common/dist/geometry/*"]
	},
	"esModuleInterop": false,
	"allowSyntheticDefaultImports": true,
	"lib": ["DOM", "ESNext"],
	"moduleResolution": "node",
	"forceConsistentCasingInFileNames": true,
	"strict": true,
	"skipLibCheck": true,
	"experimentalDecorators": true,
	"emitDecoratorMetadata": true,
	"strictPropertyInitialization": true,
	"declaration": true
	}
}